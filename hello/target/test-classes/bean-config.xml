<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- Spring Bean 설정 -->
	<bean id="firstBean" class="kr.co.greenart.MyBean"></bean>
	
	<bean id="secondBean" class="kr.co.greenart.MyBean">
		<constructor-arg value="문자열"></constructor-arg>
		<constructor-arg value="44"></constructor-arg>
	</bean>
	
	<bean id="thirdBean" class="kr.co.greenart.MyBean">
		<property name="strValue" value="TestValue"></property>
		<property name="intValue" value="77"></property>
	</bean>
</beans>
